<?xml version="1.0"?>
<configuration>
  <!-- NOTA: Cambie 'YOUR_SERVER_NAME' por el nombre de su servidor SQL Server local. -->
  <connectionStrings>
    <add name="NorthwindConnection" 
         connectionString="Data Source=YOUR_SERVER_NAME\SQLEXPRESS;Initial Catalog=Northwind;Integrated Security=True" 
         providerName="System.Data.SqlClient" />
  </connectionStrings>

  <system.web>
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime targetFramework="4.8" />
  </system.web>
</configuration>
